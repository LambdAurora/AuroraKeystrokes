  "version": "${version}",
  "description": "Displays the movement keys and mouse clicks on screen.",
  "authors": [
    "LambdAurora"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/aurorakeystrokes",
    "sources": "https://github.com/LambdAurora/AuroraKeystrokes.git",
    "issues": "https://github.com/LambdAurora/AuroraKeystrokes/issues"
  },
  "license": "MIT",
  "icon": "assets/aurora_keystrokes/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "me.lambdaurora.keystrokes.AuroraKeystrokes"
    ],
    "cotton-client-commands": [
      "me.lambdaurora.keystrokes.command.KeystrokesCommand"
    ]
  },
  "mixins": [
    "keystrokes.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.8.0",
    "fabric": "*",
    "minecraft": ">=1.16",
    "cotton-client-commands": ">=1.0.1+1.16-rc1",
